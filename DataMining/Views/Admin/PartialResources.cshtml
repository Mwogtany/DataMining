@model List<DataMining.DataAccess.proc_GetSummaryResources_Result>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.First().Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.First().Records)
        </th>
    </tr>

    @for (int i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => Model[i].Description, new { @class = "username" })
            </td>
            <td>
                @Html.DisplayFor(modelItem => Model[i].Records, new { @class = "DateSearched" })
            </td>
        </tr>
    }

</table>